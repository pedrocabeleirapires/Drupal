# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
# /web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/
/web/sites/*/tmp/

# Ignore Theme generated css to avoid merges
/web/themes/custom/*/css/style.css
/web/themes/custom/*/css/style.css.map

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/
/**/.idea/
/.vs/
/**/.vs/

# Ignore .env files as they are personal
/.env

# Ignore the build caches
.sass-cache

# Ignore .git files from modules, to avoid git subproject mess
/web/modules/contrib/**/.git
/web/themes/contrib/**/.git

# Ignore local Mac files
.DS_Store*
ehthumbs.db
Icon?
Thumbs.db
._*
node_modules
nbproject

# Archives and binaries
*.tar
*.gz
*.bz2
*.7z
*.xz
*.zip
*.rar
*.tgz
*.iso
*.jar
*.dmg
*.exe
*.so
*.dll

# Logs and databases
*.log
*.sql

# Local yarn, npm files
yarn.lock
package-lock.json

# Ignore .env in custom theam
/web/themes/custom/*/.env
/.gitattributes